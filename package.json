{
  "name": "jlivertool",
  "version": "1.2.7",
  "description": "Simple Bilibili Danmaku Tool",
  "main": "src/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "build": "electron-builder build -mw --publish=never"
  },
  "build": {
    "win": {
      "icon": "src/icons/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "src/icons/icon.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "author": "Xinrea",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^15.0.0",
    "electron-builder": "^22.14.13",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "electron-db": "^0.15.7",
    "electron-store": "^8.0.1",
    "exceljs": "^4.3.0",
    "filereader": "^0.10.3",
    "moment": "^2.29.1",
    "node-fetch": "^3.2.0",
    "openurl": "^1.1.1",
    "pako": "^2.0.4",
    "reconnecting-websocket": "^4.4.0",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  }
}
